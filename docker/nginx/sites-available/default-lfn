server {
    listen 8000 default_server;
    listen [::]:8000 default_server ipv6only=on;

    root /usr/share/nginx/html;
    index index.html index.htm;

    server_name localhost;

    location ~* /lfn-ui/ {
        root /www/static;
        expires 1d;
    }

    location / {
        root /www/static/lfn-ui;
        expires 1d;
    }

}
